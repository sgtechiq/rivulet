{
    "name": "rivulet/rivulet",
    "description": "A modern, fast, lightweight PHP framework focused on API development.",
    "version": "1.0.0",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Your Name",
            "email": "your@email.com"
        }
    ],
    "homepage": "https://yourwebsite.com/rivulet",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/sgtechiq/rivulet.git"
        }
    ],
   "require": {
        "php": "^8.3",
        "vlucas/phpdotenv": "^5.4",
        "predis/predis": "^2.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "Rivulet\\": "core/",
            "App\\": "app/"
        },
        "files": [
            "core/Support/helpers.php",
            "core/Database/Migrations/SeedOperation.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-create-project-cmd": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ]
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}